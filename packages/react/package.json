{
  "name": "@pro6pp/infer-react",
  "description": "Headless React one-field input for the Pro6PP Infer API.",
  "homepage": "https://github.com/pro6pp/infer-sdk/tree/main/packages/react",
  "keywords": [
    "react",
    "hook",
    "address",
    "autocomplete",
    "pro6pp",
    "netherlands",
    "germany",
    "postal-code",
    "validation",
    "infer-api",
    "sdk"
  ],
  "bugs": {
    "url": "https://github.com/pro6pp/infer-sdk/issues"
  },
  "sideEffects": false,
  "version": "0.0.2-beta.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "@pro6pp/infer-core": "0.0.2-beta.1"
  },
  "devDependencies": {
    "@types/react": "^19.2.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pro6pp/infer-sdk.git",
    "directory": "packages/react"
  }
}
