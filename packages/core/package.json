{
  "name": "@pro6pp/infer-core",
  "type": "module",
  "description": "Core logic and client for the Pro6PP Infer API.",
  "homepage": "https://github.com/pro6pp/infer-sdk/tree/main/packages/core",
  "keywords": [
    "pro6pp",
    "infer-api",
    "sdk",
    "core",
    "headless",
    "address",
    "autocomplete",
    "validation",
    "postal-code",
    "netherlands",
    "germany",
    "api-client",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/pro6pp/infer-sdk/issues"
  },
  "sideEffects": false,
  "version": "0.0.2-beta.9",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "script": "./dist/index.global.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pro6pp/infer-sdk.git",
    "directory": "packages/core"
  }
}
