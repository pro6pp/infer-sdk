{
  "name": "infer-js-sdk-root",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces",
    "format": "prettier --write \"./packages/**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "npm run type-check --workspaces --if-present"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
