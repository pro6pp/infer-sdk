{
  "name": "@pro6pp/infer-react",
  "type": "module",
  "description": "Headless React one-field input for the Pro6PP Infer API.",
  "homepage": "https://github.com/pro6pp/infer-sdk/tree/main/packages/react",
  "keywords": [
    "react",
    "hook",
    "address",
    "autocomplete",
    "pro6pp",
    "netherlands",
    "germany",
    "postal-code",
    "validation",
    "infer-api",
    "sdk"
  ],
  "bugs": {
    "url": "https://github.com/pro6pp/infer-sdk/issues"
  },
  "sideEffects": false,
  "version": "0.0.2-beta.11",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "@pro6pp/infer-core": "0.0.2-beta.9"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.2.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pro6pp/infer-sdk.git",
    "directory": "packages/react"
  }
}
